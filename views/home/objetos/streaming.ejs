<% if(streaming)
{
%>

<div class="video-container w3-col s12  ">

  <div class="streaming-loading"  >

<%- partial('loading-streaming.ejs')%>
  </div>

  <video  class="w3-col s12 " id="video"></video>
</div>

<script>
  if(Hls.isSupported()) {
    var video = document.getElementById('video');
    var hls = new Hls();
    hls.loadSource('/get/streaming/<%= streaming %>/out.m3u8');
    hls.attachMedia(video);
    hls.on(Hls.Events.MANIFEST_PARSED,function() {
      video.play();
    });
  }
  else
  {
    alert("cant play hls");
  }
</script>
<%}%>
